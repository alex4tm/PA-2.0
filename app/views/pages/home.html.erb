<div class="container d-flex main-landing">
  <div class="f-info">
    <h1 id="typed-text1">YouKnowMe</h1>
    <h4 id="t">You name it , We save it</h4>
    <h6>Stop wasting your time - send your preferences and information the right way.</h6>

  <%= form_for @user, url: user_session_path, :method => "post" do |f| %>
  <%= f.text_field :email, readonly: "true", hidden: "true" %>
  <%= f.password_field :password, value: '123456', readonly: "true", hidden: "true" %>
  <%= f.submit "User-demo", class: "btn landing-btn" %>
  <% end %>


<%= form_for @restaurant, url: user_session_path, :method => "post" do |f| %>
<%= f.text_field :email, readonly: "true", hidden: "true" %>
<%= f.password_field :password, value: '123456', readonly: "true", hidden: "true" %>
<%= f.submit "Restaurant-demo", class: "btn landing-btn" %>
<% end %>


  </div>
  <div class="landing-images">
    <img class="img-1" src="https://images.unsplash.com/photo-1533227268428-f9ed0900fb3b?ixid=MXwxMjA3fDB8MHxzZWFyY2h8NDR8fGhhcHB5JTIwY29kZXJ8ZW58MHwwfDB8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="">
    <img class="img-2" src="https://images.unsplash.com/photo-1534330786040-317bdb76ccff?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=832&q=80" alt="">
    <img class="img-3" src="https://images.unsplash.com/photo-1571903244693-227d7733a917?ixid=MXwxMjA3fDB8MHxzZWFyY2h8MzN8fGNoZWZ8ZW58MHx8MHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="">


  </div>
</div>
<div class=" container mt-3 valueproposition d-flex">
      <div class="value d-flex">
        <p class="value1"><i class="fab fa-wpforms"></i></p>
        <p>Sign up, add to your profile as many preferences as you wish</p>
      </div>
      <div class="value d-flex">
        <p class="value2"><i class="fas fa-share-alt-square"></i></p>
        <p>Use the unique code to share your preferences with any restaurant</p>
      </div>
      <div class="value d-flex">
        <p class="value3"><i class="fas fa-umbrella-beach"></i></p>
        <p class="mt-1">Enjoy the free time you save every time you book.</p>
      </div>
</div>




